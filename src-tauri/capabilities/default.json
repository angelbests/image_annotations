{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "window:allow-set-always-on-top",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "fs:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "dialog:allow-save",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "fs:allow-open",
    "fs:allow-read-dir",
    "fs:allow-read",
    "fs:allow-create",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "webview:allow-internal-toggle-devtools",
    {
      "identifier":"fs:allow-read",
      "allow": [{
        "path": "**"
      }]
    },
    {
      "identifier":"fs:allow-write",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-mkdir",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-exists",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-create",
      "allow": [{"path": "**"}]
    }
  ]
}