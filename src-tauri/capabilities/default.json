{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "fs:default",
    "dialog:default",

    "dialog:allow-open",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "dialog:allow-save",
    "fs:allow-lstat",
    "fs:allow-fstat",
    "fs:allow-ftruncate",
    "resources:allow-close",
    "resources:default",
    "fs:read-all",
    "fs:read-dirs",
    "fs:read-files",
    "fs:read-meta",
    "fs:scope",
    {
      "identifier":"fs:allow-read-text-file",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-read-dir",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-open",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-read",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-write",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-mkdir",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-exists",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-create",
      "allow": [{"path": "**"}]
    },

    "webview:allow-internal-toggle-devtools",
    "window:allow-set-always-on-top",
    "window:allow-set-title",
    "window:allow-set-ignore-cursor-events",
    "shell:allow-open",
    {
      "identifier":"shell:allow-execute",
      "allow": [
        {
          "name":"PaddleOCR-json",
          "cmd": "$RESOURCE\\PaddleOCR-json\\PaddleOCR-json.exe",
          "args":true,
          "sidecar": false
        }
      ]
      
    },
    {
      "identifier":"fs:allow-resource-meta",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-resource-meta-recursive",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-resource-read",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-resource-read-recursive",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-resource-write",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:allow-resource-write-recursive",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:scope-resource",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:scope-resource-index",
      "allow": [{"path": "**"}]
    },
    {
      "identifier":"fs:scope-resource-recursive",
      "allow": [{"path": "**"}]
    }

    
  ]
}