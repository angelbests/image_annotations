{
  "productName": "tauri_image_annotations",
  "version": "0.0.0",
  "identifier": "com.tauri.imageannotations",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {"windows": [
      {
        "title": "tauri_image_annotations",
        "width": 800,
        "height": 600
      }
    ],
    "withGlobalTauri": false,
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable":true,
        "scope": [
          "**","$RESOURCE\\**"
        ]
      }
    }
  },
  "bundle": {
    "resources":[
      "PaddleOCR-json\\models\\ch_PP-OCRv3_det_infer\\*",
      "PaddleOCR-json\\models\\ch_PP-OCRv3_rec_infer\\*",
      "PaddleOCR-json\\models\\*",
      "PaddleOCR-json\\*"
    ],
    "active": true,
    "targets": "all",
    "icon": [ 
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
